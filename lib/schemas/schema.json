  "info": {
    "title": "EasyMotion API",
    "description": "The EasyMotion API",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "in": "header",
        "name": "Authorization",
        "description": "Enter your Bearer token"
      }
    },
    "schemas": {
      "CreateCourseDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the course"
          },
          "description": {
            "type": "string",
            "description": "Full description of the course"
          },
          "short_description": {
            "type": "string",
            "description": "Short description of the course"
          },
          "location": {
            "type": "string",
            "description": "Location where the course is held (optional)"
          },
          "schedule": {
            "description": "Schedule of course session days",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "instructors": {
            "description": "Array of user IDs of instructors",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "category": {
            "type": "string",
            "description": "Category of the course",
            "enum": [
              "ACQUAGYM",
              "CROSSFIT",
              "PILATES",
              "ZUMBA_FITNESS",
              "POSTURAL_TRAINING",
              "BODYWEIGHT_WORKOUT"
            ]
          },
          "level": {
            "type": "string",
            "description": "Level of the course",
            "enum": [
              "BASIC",
              "MEDIUM",
              "ADVANCED"
            ]
          },
          "frequency": {
            "type": "string",
            "description": "Frequency of the course",
            "enum": [
              "SINGLE_SESSION",
              "WEEKLY",
              "MONTHLY"
            ]
          },
          "session_duration": {
            "type": "string",
            "description": "Duration of each session in POSIX format"
          },
          "cost": {
            "type": "number",
            "minimum": 1,
            "description": "Cost of the course (optional)"
          },
          "discount": {
            "type": "number",
            "minimum": 1,
            "description": "Discount for the course (optional)"
          },
          "availability": {
            "type": "string",
            "description": "Availability status of the course",
            "enum": [
              "ACTIVE",
              "COMING_SOON",
              "NO_LONGER_AVAILABLE"
            ]
          },
          "highlighted_priority": {
            "type": "number",
            "minimum": 1,
            "description": "Priority level for highlighting the course (optional)"
          },
          "members_capacity": {
            "type": "number",
            "minimum": 1,
            "description": "Maximum capacity of course members (optional)"
          },
          "num_registered_members": {
            "type": "number",
            "minimum": 1,
            "description": "Number of registered members",
            "default": 0
          },
          "tags": {
            "description": "Tags associated with the course",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "thumbnail_path": {
            "type": "string",
            "description": "Path to the thumbnail image for the course (optional)"
          }
        },
        "required": [
          "name",
          "description",
          "short_description",
          "schedule",
          "instructors",
          "category",
          "level",
          "frequency",
          "session_duration",
          "availability",
          "num_registered_members",
          "tags"
        ]
      },
